				;--------------------------------------
				; 
				; preconditions:	
				; postconditions:	
				; receives:			prompt, userInput, count, bytesRead
				; returns:			userInput, bytesRead updated
				;--------------------------------------
				mGetString MACRO prompt, userInput, count, bytesRead
					; Display a prompt
					mDisplayString prompt

					; Get user's keyboard input into a memory location
					MOV		EDX, userInput
					MOV		ECX, count
					CALL	ReadString
					MOV		userInput, EDX
					MOV		bytesRead, EAX

				ENDM


				;--------------------------------------
				; 
				; preconditions:	
				; postconditions:	
				; receives:			string (ref)
				; returns:			
				;--------------------------------------
				mDisplayString MACRO string
					PUSH	EDX
					MOV		EDX, string
					CALL	WriteString
					POP		EDX

				ENDM

