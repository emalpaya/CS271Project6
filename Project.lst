				;--------------------------------------
				; 
				; preconditions:	
				; postconditions:	
				; receives:			prompt, userInput, count, bytesRead
				; returns:			userInput, bytesRead updated
				;--------------------------------------
				;				[EBP+28], [EBP+12], [EBP+16], [EBP+8]
				mGetString MACRO mPrompt, mUserInput, mCount, mBytesRead
					; Display a prompt
					mDisplayString mPrompt

					; Get user's keyboard input into a memory location
					MOV		EDX, mUserInput
					MOV		ECX, mCount
					CALL	ReadString
					MOV		mUserInput, EDX
					MOV		mBytesRead, EAX

				ENDM


				;--------------------------------------
				; 
				; preconditions:	
				; postconditions:	
				; receives:			string (ref)
				; returns:			
				;--------------------------------------
				mDisplayString MACRO string
					PUSH	EDX
					MOV		EDX, string
					CALL	WriteString
					POP		EDX

				ENDM

